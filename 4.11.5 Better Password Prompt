var SECRET = "abc123";

function start(){
	while(true){
	    var password = readLine("What is the password? ");
	    
	    if(password != SECRET){
	        println("Sorry, that did not match. Please try again.");
	    }
	       
	    if(password == SECRET){
	        println("You got it!");
	        break;
	    }
	}
}


